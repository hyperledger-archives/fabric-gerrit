        "comments": [
          {
            "file": "CONTRIBUTING.md",
            "line": 6,
            "message": "Advice, not required actions.",
            "reviewer": {
              "email": "mbauer@us.ibm.com",
              "name": "Morgan Bauer",
              "username": "MHBauer"
