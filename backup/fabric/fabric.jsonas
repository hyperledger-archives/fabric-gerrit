            }
          },
          {
            "file": "core/ledger/kvledger/txmgmt/statedb/statecouchdb/statecouchdb.go",
            "line": 347,
            "message": "Misplaced comment... this is not the PROCESS step but the reset.",
            "reviewer": {
              "email": "enyeart@us.ibm.com",
              "name": "David Enyeart",
              "username": "denyeart"
            }
          },
          {
            "file": "core/ledger/kvledger/txmgmt/statedb/statecouchdb/statecouchdb.go",
            "line": 347,
            "message": "Done",
