            "value": "-1"
          },
          {
            "by": {
              "email": "beharrison@nc.rr.com",
              "name": "Bret Harrison",
              "username": "bretharrison"
            },
            "description": "Code-Review",
            "grantedOn": 1543590524,
            "type": "Code-Review",
